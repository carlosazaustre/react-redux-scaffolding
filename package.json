{
  "name": "react-redux-scaffolding",
  "version": "0.1.0",
  "description": "personal starter-kit for react-redux web applications",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test ava",
    "test:watch": "npm run test -- --watch",
    "serve": "webpack-dev-server",
    "clean": "rimraf ./dist && mkdir dist",
    "start": "npm-run-all --parallel serve test:watch",
    "prebuild": "npm-run-all clean test",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "postbuild": "cross-env NODE_ENV=production webpack-dev-server --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlosazaustre/react-redux-scaffolding.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "es2017"
  ],
  "author": "Carlos Azaustre (https://carlosazaustre.es/blog)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlosazaustre/react-redux-scaffolding/issues"
  },
  "homepage": "https://github.com/carlosazaustre/react-redux-scaffolding#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "npm-run-all": "^3.1.2",
    "purifycss-webpack-plugin": "^2.0.3",
    "react-addons-test-utils": "^15.4.1",
    "redux-devtools-extension": "^1.0.0",
    "redux-immutable-state-invariant": "^1.2.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "materialize-css": "^0.97.8",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
